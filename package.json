{
  "name": "pluralsight-redux-starter",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel lint:watch open:src test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node ./app.js"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.7",
    "cookie-parser": "^1.4.3",
    "http": "latest",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "3.1.1",
    "loaders.css": "^0.1.2",
    "method-override": "^2.3.7",
    "morgan": "^1.7.0",
    "querystring": "^0.2.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-loaders": "^2.3.0",
    "react-loading-spinner": "^1.0.12",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.2",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "http": "latest",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.9",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.18.0",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "jsdom": "9.8.3",
    "mocha": "3.2.0",
    "nock": "9.0.2",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/pluralsight-redux-starter"
  }
}
